<apex:page controller="appinfo">
    <apex:form >
        <apex:pageBlock >
            <apex:pageBlockTable value="{!jobapp}" var="allappsinfo">
                <apex:column >
                    <apex:outputField value="{!allappsinfo.Candidate__c}"/>
                </apex:column>
                <apex:column >
                    <apex:outputField value="{!allappsinfo.Position__r.name}"/>
                </apex:column>
            </apex:pageBlockTable>
        </apex:pageBlock>
    </apex:form>
  </apex:page>